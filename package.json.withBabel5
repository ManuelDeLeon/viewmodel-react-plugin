{
  "name": "vmc2jsx",
  "version": "0.0.0-semantically-released",
  "description": "Converts ViewModel code into React",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist",
    "commit": "git-cz",
    "commit:push": "git-cz && git push",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha src/**/*.test.js -w --compilers js:babel/register",
    "test:single": "istanbul cover -x *.test.js node_modules/mocha/bin/_mocha -- -R spec src/**/*.test.js --compilers js:babel/register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ManuelDeLeon/vmc2jsx.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "Manuel De Leon <manueldeleon@gmail.com> (https://viewmodel.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ManuelDeLeon/vmc2jsx/issues"
  },
  "homepage": "https://github.com/ManuelDeLeon/vmc2jsx#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel": "5.8.21",
    "chai": "3.3.0",
    "codecov.io": "0.1.6",
    "commitizen": "1.0.5",
    "cz-conventional-changelog": "1.1.2",
    "ghooks": "0.3.2",
    "istanbul": "0.3.21",
    "mocha": "2.2.5",
    "semantic-release": "4.3.5",
    "rimraf": "2.5.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
